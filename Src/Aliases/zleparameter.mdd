name=zleparameter

moddeps="zsh/zleparameter"

objects="zleparameter.o"

:<<\Make
zleparameter.mdd: alias.mdd.in
	rm -f $@
	cd $(sdir) && ./.preconfig

zleparameter.c: alias.c.in
	sed 's,@NAME@,zleparameter,g' < $(sdir)/alias.c.in > $@

clean-here: clean.zleparameter
clean.zleparameter:
	rm -f zleparameter.c
Make
